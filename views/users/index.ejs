<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/style/main.css" rel="stylesheet">
    <link href="/style/profile">
    <title>Users</title>
  </head>
  <body>
    <%- include('../partials/navbar') %>
    <h1>Welcome <%= user.username %></h1>
    <main>
      <section id="userMenu">
      <% use.forEach((us)=>{ %> 
        <a href="/users/<%= user._id %>/users/<%=us._id%>">
          <div class="tasks">
            <div class="profilePictures" style="background-image: url('<%=us.account.picture%>');"></div>
            <span id="names"><%=us.username%><br><%=us.account.name%></span>
            <span id="info"><%=us.account.gender%><br><%=us.account.age%><br><%=us.account.info%></span>
          </div>
        </a>
    <% }) %>
    </section>
    </main>
    
  </body>
</html>