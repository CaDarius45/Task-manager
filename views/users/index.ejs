<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/style/main.css" rel="stylesheet">
    <link href="/style/profile">
    <title>Users</title>
  </head>
  <body>
    <%- include('../partials/navbar') %>
    <h1>Welcome user</h1>
    <main>
      <section id="userMenu">
      <% users.forEach((useres)=>{ %> 
        <a href="/users/<%= user._id %>/users/<%=useres._id%>">
          <div class="tasks">
            <div class="profilePictures" style="background-image: url('<%=useres.account.picture%>');"></div>
            <%=useres.username%><br><%=useres.account.name%>
          </div>
        </a>
    <% }) %>
    </section>
    </main>
    
  </body>
</html>